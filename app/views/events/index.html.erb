<div class="page-header">
  <h1>Events</h1>
</div>

<div class="row">
  <%@events.each do |event|%>
    <div class="col-md-3">
      <div class="thumbnail">
        <%=link_to event_path(event) do%><%=image_tag event.logo.url %><%end%>
        <div class="caption">
          <h4><%=event.name%></h4>
        </div>
      </div>
    </div>
  <%end%>
</div>
